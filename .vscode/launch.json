{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch main",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "console": "externalTerminal"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/Documents/Go/go-pills/view",
      "program": "d:/Documents/Go/go-pills/view/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}